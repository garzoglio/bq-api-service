# Copy this file to .env and fill in your project-specific values.
# This file is used by `just` and shell scripts to load environment variables.
# DO NOT COMMIT the .env file to your repository.

# Your Google Cloud Project ID
PROJECT_ID="YOUR_PROJECT_ID"

# The GCP region for your resources
REGION="us-central1"

# The name of the Cloud Run service
SERVICE_NAME="bq-api-service"

# The name for the dedicated service account
SERVICE_ACCOUNT_NAME="bq-api-sa"

# The name for the Artifact Registry repository
ARTIFACT_REGISTRY_REPO="cloud-run-source-deploy"